        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Grounding the player if block is underneath
if (global.grav == 2 &amp;&amp; place_meeting(x,y+16,obj_block))
|| (global.grav == 0 &amp;&amp; place_meeting(x,y-16,obj_block))
|| (global.grav == 1 &amp;&amp; place_meeting(x+16,y, obj_block))
|| (global.grav == 3 &amp;&amp; place_meeting(x-16,y, obj_block))
{   grounded = true;    }

else grounded = false;

//Movement
if keyboard_check(ord("D")) || keyboard_check(vk_right)
{
    player_move_right(playerspeed + (3*global.playersize), global.grav);
}

if keyboard_check(ord("A")) || keyboard_check(vk_left)
{
    player_move_left(playerspeed + (3*global.playersize), global.grav); 
}

if keyboard_check_pressed(ord("W")) || keyboard_check_pressed(vk_up)
{
    if grounded{
        player_jump(jumpheight, global.grav);
    }
}

//Instance Creation
if !blobble
{
    repeat(3) instance_create(x+random(16)-8,y+random(16)-8,obj_blobble);
    alarm[0] = 3;
    blobble = true;
}

//Terminal Velocity
if phy_speed_x &gt; 12 phy_speed_x = 12;
else if phy_speed_x &lt; -12 phy_speed_x = -12;
if phy_speed_y &gt; 12 phy_speed_y = 12;
else if phy_speed_y &lt; -12 phy_speed_y = -12;
</string>
