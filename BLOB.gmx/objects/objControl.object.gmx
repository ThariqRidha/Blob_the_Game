            <string>///Init Level

//Create Asteroids
repeat (3)
{
xx = irandom_range(-100, 1380);
yy = irandom_range(50, 600);
instance_create(xx, yy, objAsteroid);
}

//Create clouds
repeat (4)
{
xx = irandom_range(-100, 1380);
yy = irandom_range(50, 600);
instance_create(xx, yy, objCloud);
}

//Init physics
physics_world_create(1/32);
physics_world_gravity(0, 0);

//Set debug flag
global.debugFlag = phy_debug_render_aabb
                 | phy_debug_render_collision_pairs
                 | phy_debug_render_coms
                 | phy_debug_render_core_shapes
                 | phy_debug_render_joints
