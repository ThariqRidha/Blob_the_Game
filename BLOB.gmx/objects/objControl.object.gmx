        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Game end checks
if instance_number(objMouse) == 0
{
if !instance_exists(objWinMenu)
    {
    instance_create(0, 0, objWinMenu);
    }
exit;
}

var numberOfCats = instance_number(objCat);
if numberOfCats == 0
{
global.pause = false;
room_restart();
}

//Open pause menu
if mouse_check_button_pressed(mb_left)
{
var mx = mouse_x;
var my = mouse_y;
if point_distance(view_xview[0] + 100, view_yview[0] + 100, mx, my) &lt; 70
    {
    if global.audio audio_play_sound(sndButton, 0, false);
    if (!instance_exists(objPawsMenu))
        {
        global.pause = true;
        instance_create(0, 0, objPawsMenu);
        }
    else
        {
        global.pause = false;
        with (objPawsMenu) instance_destroy();
        }
    }
}
else
{
//Devices backspace check
if keyboard_check_pressed(vk_backspace)
    {
    if global.audio audio_play_sound(sndButton, 0, false);
    if !instance_exists(objPawsMenu)
        {
        global.pause = true;
        instance_create(0, 0, objPawsMenu);
        }
    else
        {
        global.pause = false;
        with (objPawsMenu) instance_destroy();
        }
    }
}
</string>
